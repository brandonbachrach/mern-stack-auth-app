{
  "name": "mern-stack-auth-app",
  "version": "1.0.0",
  "description": "Full Stack Authorization App using MongoDB, Express, React/Redux, and Node.",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beb131/mern-stack-auth-app.git"
  },
  "keywords": [
    "Full-Stack",
    "Mern",
    "React",
    "Redux",
    "MongoDB",
    "Express",
    "Node"
  ],
  "author": "Brandon Bachrach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beb131/mern-stack-auth-app/issues"
  },
  "homepage": "https://github.com/beb131/mern-stack-auth-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
